public class ArraySum {
    public static void main(String[] args) {
        // Example array
        int[] numbers = {1, 2, 3, 4, 5};
        
        int sum = 0;
        // Loop through the array and add each element to sum
        for (int num : numbers) {
            sum += num;
        }
        
        // Print the result
        System.out.println("Sum of the array elements: " + sum);
    }
}
